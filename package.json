    "firebase": "^10.12.2",
    "firebase-admin": "^12.5.0",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.436.0",
    "pdf-lib": "^1.17.1",
    "query-string": "^9.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
