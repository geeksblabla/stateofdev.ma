    "@testing-library/user-event": "^14.5.2",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
